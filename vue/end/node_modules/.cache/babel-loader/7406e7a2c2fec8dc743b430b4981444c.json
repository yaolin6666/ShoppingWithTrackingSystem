FT,oBAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,6BAAE,QAAA,CAAA,GAAA,CAAI,KAAK,CAAC,GAAV,CAAF;AAAA;AAEI,mBANZ,E,IAAA,E;;AAAA,oB,WAAA,CAFuB,CACvB;AAAA,iBADgB,C;;;;eADpB,CAvC+C,CAA/C;AAAA,a;;;;WADF,E;;AAAA,Y,6BAAA,CAoDW,CApDX;AAAA,S,CADe;;;;AAAA,OAAjB,CAsDS,CAtDT;AAAA,K;;;;GADF,CAqEI,EAXJ,mBAAA,CAAA,MAAA,CAWI,EAVJ,YAAA,CAOS,iBAPT,EAOS;AAPA,IAAA,MAAM,EAAE,EAOR;AAPY,IAAA,KAAK,EAAC;AAOlB,GAPT,E;sBACE;AAAA,aAKS,CALT,YAAA,CAKS,iBALT,EAKS;AALA,QAAA,IAAI,EAAE,EAKN;AALU,QAAA,KAAK,EAAC;AAKhB,OALT,E;0BACG;AAAA,iBAGa,CAHb,YAAA,CAGa,wBAHb,EAGa;AAHU,YAAA,WAAW,EAAE,KAAA,CAAA,WAGvB;;qBAHuB,KAAA,CAAA,W,GAAW,M;cAGlC;AAHqC,0BAAY,CAAA,CAAA,EAAA,EAAA,EAAA,EAAA,EAAA,EAAA,CAGjD;AAHmE,yBAAW,KAAA,CAAA,QAG9E;AAFD,YAAA,MAAM,EAAC,yCAEN;AAFiD,YAAA,KAAK,EAAE,KAAA,CAAA,KAExD;AAFgE,YAAA,YAAW,EAAE,QAAA,CAAA,gBAE7E;AADA,YAAA,eAAc,EAAE,QAAA,CAAA;AAChB,WAHb,E,IAAA,E;;AAAA,Y,wEAAA,CAGa,CAHb;AAAA,S;;;;OADH,CAKS,CALT;AAAA,K;;;;GADF,CAUI,CAhFN,C","sourcesContent":["<template>\r\n  <div class=\"userindex\">\r\n    <div style=\"margin: 10px 0;display: flex;\">\r\n      <el-input v-model=\"search\" placeholder=\"请输入\" style=\"width: 20%;\" clearable/>\r\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"lode\">查询</el-button>\r\n      <el-popconfirm title=\"确认删除吗?\" @confirm=\"deleteBatch\">\r\n        <template #reference>\r\n          <el-button type=\"danger\">批量删除</el-button>\r\n        </template>\r\n      </el-popconfirm>\r\n    </div>\r\n    <!-- 检索结果 -->\r\n    <el-row :gutter=\"0\" class=\"userindex-list\">\r\n      <el-col :span=\"21\">\r\n        <el-table :data=\"admin\" border stripe style=\"width: 100%\" @selection-change=\"handleSelectionChange\">\r\n          <el-table-column type=\"selection\" width=\"55\" />\r\n          <el-table-column  prop=\"assessId\" label=\"ID\" width=\"40\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"customerId\" label=\"用户ID\"/>\r\n          <el-table-column  prop=\"productId\" label=\"商品ID\" width=\"70\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"productImage\" label=\"商品图片\" width=\"180\">\r\n             <template #default=\"scope\">\r\n          <el-image style=\"width: 120px; height: 120px\" :src=\"scope.row.productImage\" :preview-src-list=\"[scope.row.productImage]\">\r\n            <div solt=\"error\" class=\"image-slot\">\r\n              <i class=\"el-icon-picture-outline\"></i>\r\n            </div>\r\n          </el-image>\r\n        </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"productName\" label=\"商品名称\" width=\"150\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"productColor\" label=\"商品颜色\" width=\"75\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"productPrice\" label=\"商品价格\" width=\"80\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"productNum\" label=\"商品数量\" width=\"75\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"paymentMethod\" label=\"付款方式\" width=\"90\">\r\n          </el-table-column>\r\n           <el-table-column prop=\"shippingUser\" label=\"收货人\" width=\"90\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"address\" label=\"收货人地址\" width=\"90\">\r\n          </el-table-column>\r\n           <el-table-column prop=\"productPhones\" label=\"收货人电话\" width=\"90\">\r\n          </el-table-column>\r\n            <el-table-column prop=\"shippingCompName\" label=\"快递公司\" width=\"90\">\r\n          </el-table-column>\r\n           <el-table-column prop=\"productMessage\" label=\"订单备注\" width=\"90\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"createTime\" label=\"创建时间\" width=\"150\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"updateTime\" label=\"最后修改时间\" width=\"150\">\r\n          </el-table-column>\r\n          <el-table-column fixed=\"right\" label=\"操作\" width=\"140\" align=\"center\">\r\n            <template #default=\"scope\">\r\n              <p>等待买家评价</p>\r\n              <el-button\r\n                type=\"danger\"\r\n                icon=\"el-icon-delete\"\r\n                circle\r\n                @click=\"del(scope.row)\"\r\n              >\r\n              </el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <!-- 分页 -->\r\n    <el-row :gutter=\"20\" class=\"userindex-list\">\r\n      <el-col :span=\"20\" class=\"userindex-page-box\">\r\n         <el-pagination v-model:currentPage=\"currentPage\" :page-sizes=\"[5, 10, 20, 30]\" :page-size=\"pageSize\"\r\n                     layout=\"total, sizes, prev, pager, next, jumper\" :total=\"total\" @size-change=\"handleSizeChange\"\r\n                     @current-change=\"handleCurrentChange\">\r\n      </el-pagination>\r\n      </el-col>\r\n    </el-row>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\"\r\nimport {ElMessage} from 'element-plus'\r\nexport default {\r\n  created() {\r\n    this.lode()\r\n  },\r\n  methods: {\r\n    lode() {\r\n      request.get(\"/assess/page\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n        }\r\n      }).then(res => {\r\n        console.log(res);\r\n        this.admin = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    //删除\r\n    del(row) {\r\n      this.$confirm(\"是否确定要删除\" , {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          request\r\n              .delete(\"/assess/delete/\" + row.assessId)\r\n              .then(res => {\r\n                if (res.code === 200) {\r\n                  ElMessage({\r\n                    type: 'success',\r\n                    message: '删除成功',\r\n                  })\r\n                } else {\r\n                  ElMessage({\r\n                    type: 'error',\r\n                    message: res.msg\r\n                  })\r\n                }\r\n                this.lode()\r\n              });\r\n        })\r\n    },\r\n    deleteBatch(){\r\n      if (!this.ids.length){\r\n        ElMessage({\r\n          type:\"warning\",\r\n          message:\"请先进行选择\"\r\n        })\r\n        return\r\n      }\r\n      request.post(\"/assess/deleteBatch\",this.ids).then(res => {\r\n        if (res.code === 200) {\r\n          ElMessage({\r\n            type: 'success',\r\n            message: '批量删除成功',\r\n          })\r\n          this.lode()\r\n        } else {\r\n          ElMessage({\r\n            type: 'error',\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val){\r\n      this.ids = val.map(v => v.assessId)\r\n    },\r\n\r\n     handleSizeChange(pageSize) {//改变每页的个数触发\r\n      this.pageSize = pageSize\r\n      this.lode()\r\n\r\n    },\r\n    handleCurrentChange(pageNum) {//改变当前页码触发\r\n      this.currentPage = pageNum\r\n      this.lode()\r\n\r\n    },\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      admins:{},\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize: 5,\r\n      total: 0,\r\n      tableData: [],\r\n      admin: [],\r\n      form: {},\r\n      ids: [],\r\n    };\r\n  },\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.userindex {\r\n  width: 100%;\r\n  min-height: 100%;\r\n  padding: 15px;\r\n  box-sizing: border-box;\r\n}\r\n/* 搜索 */\r\n.userindex-queryInfo {\r\n  margin-bottom: 10px;\r\n}\r\n.userindex-queryInfo-li {\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n/* 列表 */\r\n.userindex-list {\r\n  width: 100%;\r\n  height: auto;\r\n  margin-bottom: 20px;\r\n}\r\n/* 分页 */\r\n.userindex-page-box {\r\n  width: 100%;\r\n  height: auto;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}
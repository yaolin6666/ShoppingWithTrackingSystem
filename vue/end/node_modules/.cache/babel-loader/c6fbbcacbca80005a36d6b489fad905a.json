A+C,IAA/C,CAAoD,UAAA,GAAE,EAAK;AACzD,YAAI,GAAG,CAAC,IAAJ,KAAa,GAAjB,EAAsB;AACpB,UAAA,SAAS,CAAC;AACR,YAAA,IAAI,EAAE,SADE;AAER,YAAA,OAAO,EAAE;AAFD,WAAD,CAAT;;AAIA,UAAA,MAAI,CAAC,IAAL;AACF,SANA,MAMO;AACL,UAAA,SAAS,CAAC;AACR,YAAA,IAAI,EAAE,OADE;AAER,YAAA,OAAO,EAAE,GAAG,CAAC;AAFL,WAAD,CAAT;AAIF;AACD,OAbD;AAcD,KAvHM;AAyHP,IAAA,gBAzHO,4BAyHU,QAzHV,EAyHoB;AAAC;AAC1B,WAAK,QAAL,GAAgB,QAAhB;AACA,WAAK,IAAL;AAED,KA7HM;AA8HP,IAAA,mBA9HO,+BA8Ha,OA9Hb,EA8HsB;AAAC;AAC5B,WAAK,WAAL,GAAmB,OAAnB;AACA,WAAK,IAAL;AAED;AAlIM,GANI;AA2Ib,EAAA,IA3Ia,kBA2IN;AACL,WAAO;AACL,MAAA,aAAa,EAAE,KADV;AAEL,MAAA,MAAM,EAAE,EAFH;AAGL,MAAA,MAAM,EAAE,EAHH;AAIL,MAAA,WAAW,EAAE,CAJR;AAKL,MAAA,QAAQ,EAAE,CALL;AAML,MAAA,KAAK,EAAE,CANF;AAOL,MAAA,SAAS,EAAE,EAPN;AAQL,MAAA,KAAK,EAAE,EARF;AASL,MAAA,GAAG,EAAE;AATA,KAAP;AAWD;AAvJY,CAAf","sourcesContent":["<template>\r\n  <div class=\"userindex\">\r\n    <div style=\"margin: 10px 0;display: flex;\">\r\n      <el-input v-model=\"search\" placeholder=\"请输入\" style=\"width: 20%;\" clearable/>\r\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"lode\">查询</el-button>\r\n      <el-button type=\"primary\" style=\"margin-right: 10px;\" @click=\"add\">新增</el-button>\r\n      <el-popconfirm title=\"确认删除吗?\" @confirm=\"deleteBatch\">\r\n        <template #reference>\r\n          <el-button type=\"danger\">批量删除</el-button>\r\n        </template>\r\n      </el-popconfirm>\r\n    </div>\r\n\r\n    <el-dialog title=\"收货地址\" v-model=\"dialogVisible\">\r\n      <el-form :model=\"admins\" label-width=\"100px\">\r\n        <el-form-item label=\"收货人地址\">\r\n          <el-input v-model=\"admins.goodName\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货人\">\r\n          <el-input v-model=\"admins.goodDescribe\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货人电话\">\r\n          <el-input v-model=\"admins.goodPhone\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"用户ID\">\r\n          <el-input v-model=\"admins.customerId\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"onSubmit\">确定</el-button>\r\n          <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n    <!-- 检索结果 -->\r\n    <el-row class=\"userindex-list\">\r\n      <el-col>\r\n        <el-table :data=\"admin\"\r\n                  border stripe style=\"width: 100%\" @selection-change=\"handleSelectionChange\">\r\n          <el-table-column type=\"selection\" width=\"55\"/>\r\n          <el-table-column prop=\"goodId\" label=\"ID\"/>\r\n          <el-table-column prop=\"customerId\" label=\"用户ID\"/>\r\n          <el-table-column prop=\"goodName\" label=\"收货人地址\"/>\r\n          <el-table-column prop=\"goodDescribe\" label=\"收货人名字\"/>\r\n          <el-table-column prop=\"goodPhone\" label=\"收货人电话\"/>\r\n          <el-table-column prop=\"createTime\" label=\"创建时间\"/>\r\n          <el-table-column prop=\"updateTime\" label=\"最后修改时间\"/>\r\n          <el-table-column fixed=\"right\" label=\"操作\" width=\"150\" align=\"center\">\r\n            <template #default=\"scope\">\r\n              <el-button type=\"primary\" icon=\"el-icon-edit\" circle @click=\"edit(scope.row)\"/>\r\n              <el-button type=\"danger\" icon=\"el-icon-delete\" circle @click=\"del(scope.row.goodId)\"/>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <!-- 分页 -->\r\n    <el-row :gutter=\"20\" class=\"userindex-list\">\r\n      <el-col :span=\"24\" class=\"userindex-page-box\">\r\n        <el-pagination v-model:currentPage=\"currentPage\" :page-sizes=\"[5, 10, 20, 30]\" :page-size=\"pageSize\"\r\n                       layout=\"total, sizes, prev, pager, next, jumper\" :total=\"total\" @size-change=\"handleSizeChange\"\r\n                       @current-change=\"handleCurrentChange\">\r\n        </el-pagination>\r\n      </el-col>\r\n    </el-row>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\"\r\nimport {ElMessage} from 'element-plus'\r\n\r\nexport default {\r\n  name: \"Address\",\r\n  created() {\r\n    this.lode()\r\n\r\n  },\r\n  methods: {\r\n\r\n    add() {\r\n      this.dialogVisible = true\r\n      this.admins = {}\r\n    },\r\n    onSubmit() {\r\n      if (this.admins.goodId) {\r\n        request.put('/address/update', this.admins).then(res => {\r\n          console.log(res);\r\n          if (res.code === 200) {\r\n            ElMessage({\r\n              type: 'success',\r\n              message: '修改成功',\r\n            })\r\n          } else {\r\n            ElMessage({\r\n              type: 'error',\r\n              message: res.msg\r\n            })\r\n          }\r\n          this.lode()\r\n          this.dialogVisible = false\r\n        })\r\n\r\n      } else {\r\n\r\n        request.post(\"/address/add\", this.admins).then(res => {\r\n          console.log(res);\r\n          if (res.code === 200) {\r\n            ElMessage({\r\n              type: 'success',\r\n              message: '添加成功',\r\n            })\r\n          } else {\r\n            ElMessage({\r\n              type: 'error',\r\n              message: res.msg\r\n            })\r\n          }\r\n          this.lode()\r\n          this.dialogVisible = false\r\n\r\n        });\r\n      }\r\n      ;\r\n    },\r\n\r\n    lode() {\r\n      request.get(\"/address/page\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search,\r\n        }\r\n      }).then(res => {\r\n        console.log(res);\r\n        this.admin = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n\r\n    },\r\n\r\n    //修改\r\n    edit(row) {\r\n      this.admins = JSON.parse(JSON.stringify(row))\r\n      this.dialogVisible = true\r\n    },\r\n    //删除\r\n    del(goodId) {\r\n      this.$confirm(\"是否确定要删除\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      }).then(() => {\r\n        request\r\n            .delete(\"/address/delete/\" + goodId)\r\n            .then(res => {\r\n              if (res.code === 200) {\r\n                ElMessage({\r\n                  type: 'success',\r\n                  message: '删除成功',\r\n                })\r\n              } else {\r\n                ElMessage({\r\n                  type: 'error',\r\n                  message: res.msg\r\n                })\r\n              }\r\n              this.lode()\r\n            });\r\n      })\r\n      ;\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.ids = val.map(v => v.goodId)\r\n    },\r\n    deleteBatch() {\r\n      if (!this.ids.length) {\r\n        ElMessage({\r\n          type: \"warning\",\r\n          message: \"请先进行选择\"\r\n        })\r\n        return\r\n      }\r\n      request.post(\"/address/deleteBatch\", this.ids).then(res => {\r\n        if (res.code === 200) {\r\n          ElMessage({\r\n            type: 'success',\r\n            message: '批量删除成功',\r\n          })\r\n          this.lode()\r\n        } else {\r\n          ElMessage({\r\n            type: 'error',\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n\r\n    handleSizeChange(pageSize) {//改变每页的个数触发\r\n      this.pageSize = pageSize\r\n      this.lode()\r\n\r\n    },\r\n    handleCurrentChange(pageNum) {//改变当前页码触发\r\n      this.currentPage = pageNum\r\n      this.lode()\r\n\r\n    },\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      admins: {},\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize: 5,\r\n      total: 0,\r\n      tableData: [],\r\n      admin: [],\r\n      ids: [],\r\n    };\r\n  },\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.userindex {\r\n  width: 100%;\r\n  min-height: 100%;\r\n  padding: 10px;\r\n}\r\n\r\n/* 搜索 */\r\n.userindex-queryInfo {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.userindex-queryInfo-li {\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n\r\n/* 列表 */\r\n.userindex-list {\r\n  width: 100%;\r\n  height: auto;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* 分页 */\r\n.userindex-page-box {\r\n  width: 100%;\r\n  height: auto;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}
GxB,QAAA,IAAI,EAAE;AAHkB,OAA1B,EAKK,IALL,CAKU,YAAM;AACV,QAAA,OAAO,CAAC,MAAR,CAAe,iBAAiB,KAAhC,EAAuC,IAAvC,CAA4C,UAAA,GAAE,EAAK;AACjD,UAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;;AACA,cAAI,GAAG,CAAC,IAAJ,KAAa,GAAjB,EAAsB;AACpB,YAAA,SAAS,CAAC;AACR,cAAA,IAAI,EAAE,SADE;AAER,cAAA,OAAO,EAAE;AAFD,aAAD,CAAT;AAIF,WALA,MAKO;AACL,YAAA,SAAS,CAAC;AACR,cAAA,IAAI,EAAE,OADE;AAER,cAAA,OAAO,EAAE,GAAG,CAAC;AAFL,aAAD,CAAT;AAIF;;AACA,UAAA,MAAI,CAAC,IAAL;AACD,SAdD;AAeD,OArBL;AAsBD,KAvCM;AAwCP,IAAA,WAxCO,yBAwCM;AAAA;;AACX,UAAI,CAAC,KAAK,GAAL,CAAS,MAAd,EAAqB;AACnB,QAAA,SAAS,CAAC;AACR,UAAA,IAAI,EAAC,SADG;AAER,UAAA,OAAO,EAAC;AAFA,SAAD,CAAT;AAIA;AACF;;AACA,MAAA,OAAO,CAAC,IAAR,CAAa,kBAAb,EAAgC,KAAK,GAArC,EAA0C,IAA1C,CAA+C,UAAA,GAAE,EAAK;AACpD,YAAI,GAAG,CAAC,IAAJ,KAAa,GAAjB,EAAsB;AACpB,UAAA,SAAS,CAAC;AACR,YAAA,IAAI,EAAE,SADE;AAER,YAAA,OAAO,EAAE;AAFD,WAAD,CAAT;;AAIA,UAAA,MAAI,CAAC,IAAL;AACF,SANA,MAMO;AACL,UAAA,SAAS,CAAC;AACR,YAAA,IAAI,EAAE,OADE;AAER,YAAA,OAAO,EAAE,GAAG,CAAC;AAFL,WAAD,CAAT;AAIF;AACD,OAbD;AAcD,KA9DM;AA+DP,IAAA,gBA/DO,4BA+DU,QA/DV,EA+DoB;AAAC;AAC1B,WAAK,QAAL,GAAgB,QAAhB;AACA,WAAK,IAAL;AAED,KAnEM;AAoEP,IAAA,mBApEO,+BAoEa,OApEb,EAoEsB;AAAC;AAC5B,WAAK,WAAL,GAAmB,OAAnB;AACA,WAAK,IAAL;AACD,KAvEM;AAwEP,IAAA,qBAxEO,iCAwEe,GAxEf,EAwEmB;AACxB,WAAK,GAAL,GAAW,GAAG,CAAC,GAAJ,CAAQ,UAAA,CAAA;AAAA,eAAK,CAAC,CAAC,KAAP;AAAA,OAAR,CAAX;AACD;AA1EM,GAJI;AAiFb,EAAA,IAjFa,kBAiFN;AACL,WAAO;AACL,MAAA,MAAM,EAAE,EADH;AAEL,MAAA,aAAa,EAAE,KAFV;AAGL,MAAA,WAAW,EAAE,CAHR;AAIL,MAAA,QAAQ,EAAE,CAJL;AAKL,MAAA,KAAK,EAAE,CALF;AAML,MAAA,KAAK,EAAE,EANF;AAOL,MAAA,GAAG,EAAE;AAPA,KAAP;AASD;AA3FY,CAAf","sourcesContent":["<template>\r\n  <!-- 管理员管理 -->\r\n  <div class=\"userindex\">\r\n    <div style=\"margin: 10px 0;display: flex;\">\r\n      <el-input v-model=\"search\" placeholder=\"请输入\" style=\"width: 20%;\" clearable/>\r\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"lode\">查询</el-button>\r\n      <el-popconfirm title=\"确认删除吗?\" @confirm=\"deleteBatch\">\r\n        <template #reference>\r\n          <el-button type=\"danger\">批量删除</el-button>\r\n        </template>\r\n      </el-popconfirm>\r\n    </div>\r\n\r\n    <!-- 检索结果 -->\r\n    <el-row :gutter=\"0\" class=\"userindex-list\">\r\n      <el-col :span=\"23\">\r\n        <el-table :data=\"admin\" border stripe style=\"width: 100%\" @selection-change=\"handleSelectionChange\">\r\n          <el-table-column type=\"selection\" width=\"55\"/>\r\n          <el-table-column  prop=\"imgId\" label=\"足迹ID\" width=\"100\" sortable/>\r\n          <el-table-column prop=\"customerId\" label=\"用户ID\"/>\r\n          <el-table-column  prop=\"productId\" label=\"商品ID\" width=\"80\"/>\r\n          <el-table-column prop=\"productImage\" label=\"商品图片\" width=\"180\">\r\n            <template #default=\"scope\">\r\n              <el-image style=\"width: 120px; height: 120px\" :src=\"scope.row.productImage\" :preview-src-list=\"[scope.row.productImage]\">\r\n                <div solt=\"error\" class=\"image-slot\">\r\n                  <i class=\"el-icon-picture-outline\"></i>\r\n                </div>\r\n              </el-image>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"productName\" label=\"商品名称\" width=\"250\"/>\r\n          <el-table-column prop=\"productPrice\" label=\"商品价格\" width=\"120\"/>\r\n          <el-table-column prop=\"createTime\" label=\"创建时间\" width=\"180\"/>\r\n          <el-table-column prop=\"updateTime\" label=\"最后修改时间\" width=\"180\"/>\r\n          <el-table-column fixed=\"right\" label=\"操作\" align=\"center\" width=\"140\">\r\n            <template #default=\"scope\">\r\n              <el-button\r\n                  type=\"danger\"\r\n                  icon=\"el-icon-delete\"\r\n                  circle\r\n                  @click=\"del(scope.row.imgId)\">\r\n              </el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <!-- 分页 -->\r\n    <el-row :gutter=\"20\" class=\"userindex-list\">\r\n      <el-col :span=\"22\" class=\"userindex-page-box\">\r\n        <el-pagination v-model:currentPage=\"currentPage\" :page-sizes=\"[5, 10, 20, 30]\" :page-size=\"pageSize\"\r\n                       layout=\"total, sizes, prev, pager, next, jumper\" :total=\"total\" @size-change=\"handleSizeChange\"\r\n                       @current-change=\"handleCurrentChange\">\r\n        </el-pagination>\r\n      </el-col>\r\n    </el-row>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\"\r\nimport {ElMessage} from 'element-plus'\r\nexport default {\r\n  created() {\r\n    this.lode()\r\n  },\r\n  methods: {\r\n    lode() {\r\n      request.get(\"/img/page\", {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search,\r\n        }\r\n      }).then(res => {\r\n        console.log(res);\r\n        this.admin = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n\r\n    },\r\n    //删除\r\n    del(imgId) {\r\n      this.$confirm(\"是否确定要删除\" , {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n          .then(() => {\r\n            request.delete(\"/img/delete/\" + imgId).then(res => {\r\n              console.log(imgId);\r\n              if (res.code === 200) {\r\n                ElMessage({\r\n                  type: 'success',\r\n                  message: '删除成功',\r\n                })\r\n              } else {\r\n                ElMessage({\r\n                  type: 'error',\r\n                  message: res.msg\r\n                })\r\n              }\r\n              this.lode()\r\n            })\r\n          })\r\n    },\r\n    deleteBatch(){\r\n      if (!this.ids.length){\r\n        ElMessage({\r\n          type:\"warning\",\r\n          message:\"请先进行选择\"\r\n        })\r\n        return\r\n      }\r\n      request.post(\"/img/deleteBatch\",this.ids).then(res => {\r\n        if (res.code === 200) {\r\n          ElMessage({\r\n            type: 'success',\r\n            message: '批量删除成功',\r\n          })\r\n          this.lode()\r\n        } else {\r\n          ElMessage({\r\n            type: 'error',\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(pageSize) {//改变每页的个数触发\r\n      this.pageSize = pageSize\r\n      this.lode()\r\n\r\n    },\r\n    handleCurrentChange(pageNum) {//改变当前页码触发\r\n      this.currentPage = pageNum\r\n      this.lode()\r\n    },\r\n    handleSelectionChange(val){\r\n      this.ids = val.map(v => v.imgId)\r\n    },\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      search: '',\r\n      dialogVisible: false,\r\n      currentPage: 1,\r\n      pageSize: 5,\r\n      total: 0,\r\n      admin: [],\r\n      ids: [],\r\n    };\r\n  },\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.userindex {\r\n  width: 100%;\r\n  min-height: 100%;\r\n  padding: 15px;\r\n  box-sizing: border-box;\r\n}\r\n/* 搜索 */\r\n.userindex-queryInfo {\r\n  margin-bottom: 10px;\r\n}\r\n.userindex-queryInfo-li {\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n/* 列表 */\r\n.userindex-list {\r\n  width: 100%;\r\n  height: auto;\r\n  margin-bottom: 20px;\r\n}\r\n/* 分页 */\r\n.userindex-page-box {\r\n  width: 100%;\r\n  height: auto;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}
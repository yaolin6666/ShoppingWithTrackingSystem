e()\r\n        } else {\r\n          ElMessage({\r\n            type: 'error',\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val){\r\n      this.ids = val.map(v => v.assessId)\r\n    },\r\n\r\n     handleSizeChange(pageSize) {//改变每页的个数触发\r\n      this.pageSize = pageSize\r\n      this.lode()\r\n\r\n    },\r\n    handleCurrentChange(pageNum) {//改变当前页码触发\r\n      this.currentPage = pageNum\r\n      this.lode()\r\n\r\n    },\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      admins:{},\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize: 5,\r\n      total: 0,\r\n      tableData: [],\r\n      admin: [],\r\n      form: {},\r\n      ids: [],\r\n    };\r\n  },\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.userindex {\r\n  width: 100%;\r\n  min-height: 100%;\r\n  padding: 15px;\r\n  box-sizing: border-box;\r\n}\r\n/* 搜索 */\r\n.userindex-queryInfo {\r\n  margin-bottom: 10px;\r\n}\r\n.userindex-queryInfo-li {\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n/* 列表 */\r\n.userindex-list {\r\n  width: 100%;\r\n  height: auto;\r\n  margin-bottom: 20px;\r\n}\r\n/* 分页 */\r\n.userindex-page-box {\r\n  width: 100%;\r\n  height: auto;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n</style>\r\n"]}]}